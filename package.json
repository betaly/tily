{
  "name": "tily",
  "version": "2.14.4",
  "description": "Fast, small and purely functional utility library",
  "author": "TY",
  "repository": "https://github.com/betaly/tily",
  "bugs": "https://github.com/betaly/tily/issues",
  "keywords": [
    "utils",
    "ramda",
    "lodash",
    "underscore",
    "curry",
    "performance"
  ],
  "sideEffects": false,
  "license": "Apache-2.0",
  "scripts": {
    "copy:assets": "cpy 'package.json' 'LICENSE' 'README.md' 'CHANGELOG.md' dist",
    "version": "yarn copy:assets",
    "prerelease": "cross-env CI=1 yarn build && yarn test",
    "release": "yarn prerelease && np --no-2fa --no-yarn --no-tests --contents ./dist",
    "build": "tsc -p tsconfig.json && yarn copy:assets",
    "prettier:cli": "prettier \"**/*.ts\" \"**/*.js\" \"**/*.md\"",
    "prettier:check": "yarn prettier:cli --check",
    "prettier:fix": "yarn prettier:cli --write",
    "benchmark": "ts-node --project tsconfig.benchmark.json benchmark.ts",
    "docs": "typedoc && touch docs/.nojekyll",
    "docs:clean": "rm -rf docs",
    "docs:rebuild": "yarn docs:clean && yarn docs",
    "docs:serve": "yarn docs:rebuild && hs docs -o",
    "docs:deploy": "ts-node scripts/docs.ts",
    "clean": "nb-clean 'dist' '*.tsbuildinfo'",
    "pretest": "yarn clean && yarn build",
    "posttest": "yarn prettier:check",
    "test": "yarn pretest && jest --coverage && yarn posttest",
    "test:coverage": "jest --collectCoverage --coverageDirectory=\"./coverage\" --reporters=default --reporters=jest-junit --watchAll=false"
  },
  "devDependencies": {
    "@nutol/build": "^0.2.6",
    "@types/fs-extra": "^9.0.13",
    "@types/gh-pages": "^3.2.1",
    "@types/jest": "^27.4.1",
    "benchmark": "^2.1.4",
    "conditional-type-checks": "^1.0.5",
    "coveralls": "^3.1.1",
    "cross-env": "^7.0.3",
    "gh-pages": "^3.2.3",
    "http-server": "^14.1.0",
    "jest": "^27.5.1",
    "jest-junit": "^13.0.0",
    "jsdom": "^19.0.0",
    "lazy.js": "^0.5.1",
    "lodash": "^4.17.21",
    "prettier": "^2.5.1",
    "ramda": "^0.28.0",
    "react": "^17.0.2",
    "react-is": "^17.0.2",
    "ts-expect": "^1.3.0",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.6.0",
    "typedoc": "^0.22.12",
    "typescript": "^4.6.2",
    "underscore": "^1.13.2",
    "walker": "^1.0.8",
    "zx": "^5.2.0"
  },
  "peerDependencies": {
    "react-is": "*"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "**/*"
  ],
  "packageManager": "yarn@3.2.0"
}
