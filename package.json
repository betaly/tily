{
  "name": "tily",
  "version": "2.14.1",
  "description": "Fast, small and purely functional utility library",
  "author": "TY",
  "repository": "https://github.com/betaly/tily",
  "bugs": "https://github.com/betaly/tily/issues",
  "keywords": [
    "utils",
    "ramda",
    "lodash",
    "underscore",
    "curry",
    "performance"
  ],
  "sideEffects": false,
  "license": "Apache-2.0",
  "scripts": {
    "copy:assets": "cpy package.json .npmignore LICENSE README.md CHANGELOG.md dist",
    "version": "npm run copy:assets",
    "prerelease": "cross-env CI=1 npm run build && npm run test",
    "release": "np --no-2fa --no-tests --contents ./dist",
    "build": "tsc -p tsconfig.json && npm run copy:assets",
    "prettier:cli": "prettier \"**/*.ts\" \"**/*.js\" \"**/*.md\"",
    "prettier:check": "npm run prettier:cli -- --check",
    "prettier:fix": "npm run prettier:cli -- --write",
    "benchmark": "ts-node --project tsconfig.benchmark.json benchmark.ts",
    "prebenchmark": "npm install --no-save ts-node lodash ramda lazy.js underscore",
    "docs": "typedoc && touch docs/.nojekyll",
    "docs:clean": "rm -rf docs",
    "docs:rebuild": "npm run docs:clean && npm run docs",
    "docs:serve": "npm run docs:rebuild && hs docs -o",
    "docs:deploy": "ts-node scripts/docs.ts",
    "pretest": "npm run prettier:fix",
    "test": "jest --coverage",
    "test:ci": "npm run test -- --maxWorkers=4 && cat ./coverage/lcov.info | coveralls",
    "test:ci:coveralls": "npm run test -- --maxWorkers=4"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/gh-pages": "^3.2.0",
    "@types/jest": "^27.0.3",
    "benchmark": "^2.1.4",
    "conditional-type-checks": "^1.0.5",
    "coveralls": "^3.1.1",
    "cross-env": "^7.0.3",
    "gh-pages": "^3.2.3",
    "http-server": "^14.1.0",
    "jest": "^27.4.3",
    "jsdom": "^19.0.0",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-is": "^17.0.2",
    "ts-expect": "^1.3.0",
    "ts-jest": "^27.1.0",
    "ts-node": "^10.4.0",
    "typedoc": "^0.22.11",
    "typescript": "^4.5.2",
    "walker": "^1.0.8",
    "zx": "^4.3.0"
  },
  "peerDependencies": {
    "react-is": "*"
  },
  "files": [
    "**/*"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
}
