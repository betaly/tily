{
  "name": "tily",
  "version": "2.7.0",
  "author": "Mindary",
  "scripts": {
    "release": "node scripts/release.js",
    "benchmark": "ts-node --project tsconfig.benchmark.json benchmark.ts",
    "prebenchmark": "npm install --no-save ts-node lodash ramda lazy.js underscore",
    "docs:gh": "node ./generate/docs/gh.js",
    "docs:gl": "node ./generate/docs/gl.js",
    "test": "jest --coverage",
    "test:ci": "npm run test -- --maxWorkers=4 && cat ./coverage/lcov.info | coveralls"
  },
  "description": "Fast, small and purely functional utility library",
  "repository": "https://github.com/mindary/tily",
  "bugs": "https://github.com/mindary/tily/issues",
  "keywords": [
    "mindary",
    "utils",
    "ramda",
    "lodash",
    "underscore",
    "curry",
    "performance"
  ],
  "sideEffects": false,
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.5",
    "@babel/plugin-transform-modules-commonjs": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@types/fs-extra": "^9.0.11",
    "@types/jest": "^26.0.23",
    "babel-core": "^6.26.3",
    "benchmark": "^2.1.4",
    "chalk": "^4.1.1",
    "coveralls": "^3.1.0",
    "esdoc": "^1.1.0",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-importpath-plugin": "^1.0.2",
    "esdoc-standard-plugin": "^1.0.0",
    "execa": "^5.1.1",
    "fs-extra": "^10.0.0",
    "gh-pages": "^3.2.0",
    "inquirer": "^8.1.0",
    "jest": "^27.0.4",
    "jsdom": "^16.6.0",
    "ora": "^5.4.1",
    "prettier": "^2.3.1",
    "react": "^17.0.2",
    "react-is": "^16.13.1",
    "recursive-readdir-sync": "^1.0.6",
    "ts-expect": "^1.3.0",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.2",
    "walker": "^1.0.7"
  },
  "peerDependencies": {
    "react-is": "*"
  }
}
